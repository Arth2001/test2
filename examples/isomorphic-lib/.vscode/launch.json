{
  "version": "0.2.0",
  "configurations": [
    // {
    //   "type": "node",
    //   "request": "launch",
    //   "name": "Launch Program",
    //   "program": "${workspaceFolder}/dist/run.js"
    // },
    {
      "type": "node",
      "request": "launch",
      "remoteRoot": "${workspaceRoot}/dist",
      "name": "Lanuch nodemon debugger",
      "runtimeExecutable": "npm-run",
      "runtimeArgs": [
        "nodemon"
      ],
      "program": "${workspaceFolder}/dist/run.js",
      "restart": true,
      "sourceMaps": true,
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
