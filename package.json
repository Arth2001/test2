  "tnp": {
    "resources": [
      "README.md",
      "projects",
      "bin",
      "logo_github.png",
      "logo.png",
      "files.png"
    ],
    "type": "isomorphic-lib",
    "overrided": {
      "ignoreDepsPattern": [
        "@angular.*"
      ],
      "dependencies": {
        "@ngx-formly/core": "<5",
        "@angular/core": "5",
        "rxjs": "^5.5.6",
