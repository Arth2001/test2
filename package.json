{
  "name": "morphi",
  "version": "1.0.8",
  "description": "Isomorphic solution for expressjs and angular5",
  "main": "index.js",
  "scripts": {
    "release:path": "npm version patch && tnp build:bundle && tnp --copy-resources && cd bundle && npm publish && cd ..",
    "build:watch": "npm-run watch \"morphi build\" src",
    "build": "./node_modules/.bin/ts-node ./node_modules/typescript-npm-project/index.ts",
    "build:publish": "./node_modules/.bin/ts-node ./node_modules/typescript-npm-project/index.ts --publish",
    "build:clean": "./node_modules/.bin/ts-node ./node_modules/typescript-npm-project/index.ts --clean"
  },
  "tnp": {
    "type": "isomorphic-lib",
    "resources": [
      "README.md",
      "examples",
      "bin",
      "logo_github.png",
      "logo.png",
      "files.png"
    ]
  },
  "bin": {
    "morphi": "bin/morphi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/morphi.git"
  },
  "author": "Dariusz Filipiak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darekf77/morphi/issues"
  },
  "homepage": "https://github.com/darekf77/morphi#readme",
  "dependencies": {
    "@types/express": "^4.11.0",
    "@types/fs-extra": "^5.0.1",
    "@types/lodash": "^4.14.85",
    "@types/socket.io": "^1.4.31",
    "accepts": "^1.3.4",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "check-node-version": "^3.2.0",
    "circular-json": "^0.5.1",
    "command-exists": "^1.2.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cpr": "^3.0.1",
    "dateformat": "^3.0.3",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "json5": "^0.5.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.10",
    "ng2-rest": "^9.0.11",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "socket.io": "^2.0.4",
    "tslib": "^1.8.1",
    "typeorm": "^0.1.9"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.9.3",
    "typescript": "2.6.2",
    "webpack": "3.10.0"
  }
}
