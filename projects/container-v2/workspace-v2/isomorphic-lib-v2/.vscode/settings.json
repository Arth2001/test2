{
  "files.exclude": {
    "webpack.backend-bundle-build.js": true,
    "node_modules": true,
    "tmp*": true,
    "dist*": true,
    "bundle*": true,
    "browser": true,
    "browser*": true,
    "module*": true,
    "backup": true,
    "module": true,
    "www": true,
    "Thumbs.db": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    "npm-debug.log*": true,
    ".sass-cache": true,
    ".sourcemaps": true,
    "tsconfig.isomorphic.json": true,
    "tsconfig.browser.json": true,
    ".vscode/launch.json": false,
    ".vscode/tasks.json": false,
    "tsconfig.json": true,
    "src-for-stanalone": true,
    "components-for-stanalone": true,
    "src/assets/angular-lib-v2": true,
    ".gitignore": true,
    ".npmignore": true,
    ".npmrc": true,
    ".babelrc": true,
    "docs": true,
    "logo.svg": true,
    "**/*.filetemplate": true,
    "**/tsconfig.*": true,
    "tslint.*": true,
    "index.*": true,
    "package-lock.json": true,
    "protractor.conf.js": true,
    "karma.conf.js": true,
    ".editorconfig": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#0084ff",
    "activityBar.foreground": "#000000"
  },
  "editor.rulers": [
    100,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "tslint.autoFixOnSave": true,
  "eslint.enable": false,
  "tslint.enable": false,
  "typescript.tsdk": "node_modules/typescript/lib"
}